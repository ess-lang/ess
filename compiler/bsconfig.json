{
  "name": "ess",
  "refmt": 3,
  "bsc-flags": ["-bs-super-errors"],
  "ocamlfind-dependencies": [
    "menhirlib",
    "visitors.ppx",
    "visitors.runtime",
    "ppx_deriving"
  ],
  "entries": [
    {
      "backend": "native",
      "main-module": "Index"
    }
  ],
  "sources": [
    {
      "dir": "src",
      "generators": [
        {
          "name": "ocamllex",
          "edge": ["Lexer.ml", ":", "Lexer.mll"]
        },
        {
          "name": "menhir",
          "edge": ["Parser.ml", "Parser.mli", ":", "Parser.mly"]
        }
      ]
    }
  ],
  "generators": [
    {
      "name": "ocamllex",
      "command": "ocamllex $in"
    },
    {
      "name": "menhir",
      "command": "menhir --explain --table $in"
    }
  ]
}
