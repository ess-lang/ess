{
  "name": "ess",
  "version": "0.1.0",
  "refmt": 3,
  "bsc-flags": ["-bs-super-errors"],
  "sources": [
    {"dir": "src",
      "generators" : [
      {
        "name": "ocamllex",
        "edge": ["Lexer.ml", ":", "Lexer.mll"]
      },
      {
        "name" : "menhir",
        "edge" : ["Parser.ml", "Parser.mli", ":", "Parser.mly"]
      }
      ]
    },
    {
      "dir": "__tests__",
      "type": "dev"
    },
  ],
  "generators" : [
    { "name" : "ocamllex",
      "command" : "ocamllex $in"
    },
    { "name" : "menhir" ,
      "command" : "menhir --explain $in" },
  ],
  "bs-dependencies" : [],
  "bs-dev-dependencies": [
    "bs-jest"
  ]
}
